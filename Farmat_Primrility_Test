import java.util.* ; 
public class Power{
   public static long pow(long val , long p , long mod){
       long res = 1 ; 
       while(p != 0){
           if(p % 2 == 1){
               res = (res * val) % mod ; 
           }
           val  = ( val * val ) % mod ;
           p /= 2 ; 
       }
       return res;  
   }
   public static boolean commonDivisor(long n){
       if(n <= 1 )return false ; 
       if(n == 2 || n == 3)return true ; 
       Random rand = new Random(); 
       for(int i = 1 ; i <= 5 ; i ++){
           long val = 2 + rand.nextLong(n - 2) ; 
           if(pow(val, n-1 , n) % n != 1)return false ;  
       }
       return true ; 
   }
    public static void main(String []args){
        Scanner sc =new Scanner(System.in); 
        int t = sc.nextInt() ; 
        while(t --> 0){
            long n1 = sc.nextLong() ; 
        System.out.println(commonDivisor(n1) == true  ? "YES" : "NO") ; 
        }
        
    }
}
